  "version": "0.2.0",
  "configurations": [
    {
      "type": "by-gdb",
      "request": "launch",
      "name": "Launch(remote)",
      "program": "${workspaceFolder}/build/kernel-x86_64.bin",
      "cwd": "${workspaceRoot}",
      "remote": {
        "enabled": true,
        "address": ":1234",
        "mode": "remote",
        "execfile": "${workspaceFolder}/build/kernel-x86_64.bin"
      },
      "preLaunchTask": "QEMU Debug Run"
    }
  ]
}
